{
  "name": "figma-json-plugin",
  "version": "0.0.1",
  "description": "Dump a hierarchy to JSON within a Figma document. Intended for use within Figma plugins.",
  "main": "dist/main.js",
  "browser": "dist/browser.js",
  "author": "Andrew Pouliot",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack --mode=development --watch",
    "build": "NODE_ENV=production webpack --mode=production",
    "clean": "rm -rf dist",
    "release:patch": "npm version patch && npm run build && npm publish",
    "release:dev": "npm version prerelease && npm run build && npm publish --tag dev"
  },
  "files": [
    "dist/main.js",
    "dist/src"
  ],
  "devDependencies": {
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "react-json-view": "^1.19.1"
  }
}
